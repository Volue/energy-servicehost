    <wsdl:part name="fault" element="mades:ReceiveMessageError">
      <wsdl:documentation>
				A definition of an error output message for the ReceiveMessage method
			</wsdl:documentation>
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="CheckMessageStatusFault">
    <wsdl:part name="fault" element="mades:CheckMessageStatusError">
      <wsdl:documentation>
				A definition of an error output message for the CheckMessageStatus method
			</wsdl:documentation>
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="ConfirmReceiveMessageFault">
    <wsdl:part name="fault" element="mades:ConfirmReceiveMessageError">
      <wsdl:documentation>
				A definition of an error output message for the ConfirmReceiveMessage method
			</wsdl:documentation>
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="ConfirmReceiveMessageResponse">
    <wsdl:part name="parameters" element="mades:ConfirmReceiveMessageResponse">
