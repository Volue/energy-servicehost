<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:mades="http://mades.entsoe.eu/" name="MadesEndpoint" targetNamespace="http://mades.entsoe.eu/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:types>
    <xsd:schema targetNamespace="http://mades.entsoe.eu/">
      <xsd:element name="SendMessageRequest">
        <xsd:annotation>
          <xsd:documentation>The definition of an input web service request message for a SendMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="message" type="mades:SentMessage">
              <xsd:annotation>
                <xsd:documentation>A representation of a sent message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="conversationID" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique send message request identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="SendMessageResponse">
        <xsd:annotation>
          <xsd:documentation> A definition of a web service response message for the SendMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique message ID that is automatically assigned to sent message </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="SendMessageError">
        <xsd:annotation>
          <xsd:documentation>A definition of an error output message for the SendMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The code representing the kind of the error (e.g. validation error, unexpected error etc)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique identification of the failure. The errorID shall be always used to track the error in the application logs. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Error message usually describing the reason of failure</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="receiverCode" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The receiver code of the original sent message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="errorDetails" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Additional error details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReceiveMessageRequest">
        <xsd:annotation>
          <xsd:documentation>The definition of an input web service request message for a ReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="businessType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The business type of messages to download. Only message of given business type will be downloaded. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="downloadMessage" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>Flag indicating whether to download also a payload (content) of the message. When set to "true" payload will be downloaded, otherwise it will be left in
									messagebox.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReceiveMessageResponse">
        <xsd:annotation>
          <xsd:documentation>A definition of a web service response message for the ReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="receivedMessage" nillable="true" type="mades:ReceivedMessage">
              <xsd:annotation>
                <xsd:documentation>Received message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="remainingMessagesCount" type="xsd:long">
              <xsd:annotation>
                <xsd:documentation>Count of remaining messages of the business type that was specified as input parameter of ReceiveMessage method. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReceiveMessageError">
        <xsd:annotation>
          <xsd:documentation>A definition of an error output message for the ReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The code representing the kind of the error (e.g. validation error, unexpected error etc)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique identification of the failure. The errorID shall be always used to track the error in the application logs. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Error message usually describing the reason of failure</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="businessType" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Business type that was given as input parameter in the failed of the ReceiveMessage</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="errorDetails" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Additional error details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConfirmReceiveMessageRequest">
        <xsd:annotation>
          <xsd:documentation>The definition of an input web service request message for the ConfirmReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique ID of the message. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConfirmReceiveMessageResponse">
        <xsd:annotation>
          <xsd:documentation>A definition of a web service response message for the ConfirmReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>ID of the confirmed message. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConfirmReceiveMessageError">
        <xsd:annotation>
          <xsd:documentation>A definition of an error output message for the ConfirmReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The code representing the kind of the error (e.g. validation error, unexpected error etc)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique identification of the failure. The errorID shall be always used to track the error in the application logs. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Error message usually describing the reason of failure</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="messageID" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique ID of the message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="errorDetails" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Additional error details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="SentMessage">
        <xsd:annotation>
          <xsd:documentation>A representation of a message sent via SendMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="receiverCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Endpoint code of receiver.
								May contain alphanumerical characters 'A'-'Z', 'a'-'z', '0'-'9' and additional characters '-' and '@'.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="businessType" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Code of message's business type.
								Only alphanumerical characters are allowed.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="content" type="xsd:base64Binary">
            <xsd:annotation>
              <xsd:documentation>A payload of the message.
								Any text or binary data are allowed. Please refer to MADES documentation for the maximum message size limitation.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="senderApplication" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional identification of sending business application.
								Only alphanumerical characters are allowed.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="baMessageID" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional ID of message assigned by business application.
								Only alphanumerical characters are allowed.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ReceivedMessage">
        <xsd:annotation>
          <xsd:documentation>Representation of message received by ReceiveMessage method</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="messageID" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Unique ID of the message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="receiverCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Endpoint code of the message's receiver</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="senderCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Endpoint code of message's sender</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="businessType" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>The message's business type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="content" type="xsd:base64Binary">
            <xsd:annotation>
              <xsd:documentation>The message's payload (content)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="senderApplication" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional identification of a business application on the sender's side</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="baMessageID" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional ID of message assigned by business application.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MessageStatus">
        <xsd:sequence>
          <xsd:element name="messageID" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Unique ID of the message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="state" type="mades:MessageState">
            <xsd:annotation>
              <xsd:documentation>The current state of the message, represented by a value from MessageState enumeration</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="receiverCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Endpoint code of the receiver</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="senderCode" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Endpoint code of the sender</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="businessType" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>The message's business type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="senderApplication" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional identification of a business application on the sender's side</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="baMessageID" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Optional ID of message assigned by business application</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sendTimestamp" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Exact date and time when message was sent</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="receiveTimestamp" nillable="true" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Exact date and time when message was received</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="trace" nillable="true" type="mades:MessageTrace">
            <xsd:annotation>
              <xsd:documentation>Detailed information about message's traversal through MADES network</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MessageTrace">
        <xsd:annotation>
          <xsd:documentation>Detailed information about message traversal through MADES network</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" name="trace" type="mades:MessageTraceItem" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="MessageTraceItem">
        <xsd:annotation>
          <xsd:documentation>Represents an event during the message traversal across the MADES network</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="timestamp" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Exact date and time of the event</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="state" type="mades:MessageTraceState" />
          <xsd:element name="component" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>The code of MADES messaging component on which the event happened</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="componentDescription" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Human-understandable description of the messaging component</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="details" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Details about the event</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="ConnectivityTestRequest">
        <xsd:annotation>
          <xsd:documentation> The definition of an input web service request message for the ConnectivityTest method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="receiverCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Endpoint code of receiver</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="businessType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The business type of messages to test the connectivity. Business type is important in determining the message path.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConnectivityTestResponse">
        <xsd:annotation>
          <xsd:documentation>A definition of a web service response message for the ConnectivityTest method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique message ID assigned to message.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConnectivityTestError">
        <xsd:annotation>
          <xsd:documentation>A definition of an error output message for the ConnectivityTest method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The code representing the kind of the error (e.g. validation error, unexpected error etc)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique identification of the failure. The errorID shall be always used to track the error in the application logs. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Error message usually describing the reason of failure</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="receiverCode" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Endpoint code of receiver given as input parameter of the ConnectivityTest method</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="errorDetails" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Additional error details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CheckMessageStatusRequest">
        <xsd:annotation>
          <xsd:documentation>The definition of an input web service request message for the CheckMessageStatus method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique ID of the message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CheckMessageStatusResponse">
        <xsd:annotation>
          <xsd:documentation>A definition of a web service response message for the CheckMessageStatus method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="messageStatus" type="mades:MessageStatus" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CheckMessageStatusError">
        <xsd:annotation>
          <xsd:documentation>A definition of an error output message for the CheckMessageStatus method</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="errorCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The code representing the kind of the error (e.g. validation error, unexpected error etc)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorID" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique identification of the failure. The errorID shall be always used to track the error in the application logs. </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Error message usually describing the reason of failure</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="messageID" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Unique ID of the message</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="errorDetails" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Additional error details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:simpleType name="MessageState">
        <xsd:annotation>
          <xsd:documentation>Enumeration of possible message states</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="VERIFYING">
            <xsd:annotation>
              <xsd:documentation>The message is waiting for receiver verification due to connectivity problems between messaging component and Directory Service.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ACCEPTED">
            <xsd:annotation>
              <xsd:documentation>The message has been accepted by the MADES component and stored in the messagebox.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DELIVERING">
            <xsd:annotation>
              <xsd:documentation>The message has been successfully passed to another MADES component. At the moment, this MADES component has not yet obtained further information about message delivery into
								endpoint of the recipient.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DELIVERED">
            <xsd:annotation>
              <xsd:documentation>The message has been delivered to the target endpoint.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="RECEIVED">
            <xsd:annotation>
              <xsd:documentation>The message has been received by target business application or user.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FAILED">
            <xsd:annotation>
              <xsd:documentation>The processing of the message failed. </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="MessageTraceState">
        <xsd:annotation>
          <xsd:documentation>Enumeration of possible message states for tracing purposes</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="VERIFYING" />
          <xsd:enumeration value="ACCEPTED" />
          <xsd:enumeration value="TRANSPORTED" />
          <xsd:enumeration value="DELIVERED" />
          <xsd:enumeration value="RECEIVED" />
          <xsd:enumeration value="FAILED" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:schema>
  </wsdl:types>
  <wsdl:message name="ReceiveMessageResponse">
    <wsdl:part name="parameters" element="mades:ReceiveMessageResponse">
      <wsdl:documentation>
