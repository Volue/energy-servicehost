    <wsdl:part name="parameters" element="mades:ConnectivityTestResponse">
      <wsdl:documentation>
				A definition of a web service response message for the ConnectivityTest method
			</wsdl:documentation>
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="SendMessageResponse">
    <wsdl:part name="parameters" element="mades:SendMessageResponse">
      <wsdl:documentation>
				A definition of a web service response message for the SendMessage method
			</wsdl:documentation>
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="SendMessageFault">
    <wsdl:part name="fault" element="mades:SendMessageError">
